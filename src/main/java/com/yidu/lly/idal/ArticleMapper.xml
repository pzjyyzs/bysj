<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.yidu.lly.mapper.ArticleMapper" >
  <insert id="insertArticle" parameterType="com.yidu.lly.model.Article">
  	insert into article(aid,articlename,articletime,articlecontent,uid,articleread)
  		values(#{aid},#{articlename},#{articletime},#{articlecontent},#{uid},#{articleread})
  </insert>
  </mapper>